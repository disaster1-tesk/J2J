{
    "input": {
        "numbers": [1, 2, 3, 4, 5],
        "text": "Hello World",
        "value": 16.75,
        "timestamp": 1609459200000,
        "price": 99.99,
        "items": ["apple", "banana", "cherry", "apple"]
    },
    "spec": {
        "mathOperations": {
            "multiplication": "=multiply(@(1,numbers))",
            "squareRoot": "=sqrt(@(1,value))",
            "ceiling": "=ceil(@(1,price))",
            "floor": "=floor(@(1,price))",
            "rounded": "=round(@(1,price))",
            "power": "=pow(2, 3)",
            "modulo": "=mod(10, 3)"
        },
        "stringOperations": {
            "replaced": "=replace(@(1,text), 'World', 'Universe')",
            "upperCase": "=toUpper(@(1,text))",
            "length": "=stringLength(@(1,text))",
            "charAt": "=charAt(@(1,text), 0)",
            "contains": "=stringContains(@(1,text), 'Hello')",
            "startsWith": "=startsWith(@(1,text), 'Hello')",
            "endsWith": "=endsWith(@(1,text), 'World')",
