            "lastElement": "=lastElement(@(1,numbers))",
            "joined": "=listJoin('-', @(1,items))"
        },
        "objectOperations": {
            "isNumber": "=isNumber(@(1,value))",
            "isString": "=isString(@(1,text))",
            "isList": "=isList(@(1,numbers))",
            "toString": "=coerceToString(@(1,value))",
            "defaultValue": "=defaultIfNull(@(1,missing), 'default')"
        },
        "dateOperations": {
            "currentTime": "=now",
            "formattedDate": "=formatDate('yyyy-MM-dd', @(1,timestamp))",
            "addedDays": "=addDays(1, @(1,timestamp))"
        }
    },
